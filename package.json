{
  "name": "n8n-nodes-mollie",
  "version": "1.0.1",
  "description": "n8n node for Mollie payment API with OAuth and API Key authentication",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "mollie",
    "payments"
  ],
  "license": "MIT",
  "homepage": "https://docs.mollie.com/reference/authentication",
  "author": {
    "name": "makeitfuture",
    "email": "dev@makeitfuture.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/makeitfutureDev/mollie.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/MollieApi.credentials.js",
      "dist/credentials/MollieOAuth2Api.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Mollie/Mollie.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.0.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
